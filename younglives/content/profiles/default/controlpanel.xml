<?xml version="1.0"?>
<object
    name="portal_controlpanel"
    xmlns:i18n="http://xml.zope.org/namespaces/i18n"
    i18n:domain="younglives.policy"
    purge="False">
    
    <configlet
        title="Captured data settings"
        action_id="younglives.captureddatasettings"
        appId="younglives.content" 
        category="Products"
        condition_expr=""
        url_expr="string:${portal_url}/@@captured-data-settings"
        icon_expr="string:${portal_url}/++resource++younglives.content.images/icon_captured_data_settings.png"
        visible="True"
        i18n:attributes="title">
        <permission>Manage portal</permission>
    </configlet>

    <configlet
        title="Staff Intranet users settings"
        action_id="younglives.intranetuserssettings"
        appId="younglives.content" 
        category="Products"
        condition_expr=""
        url_expr="string:${portal_url}/intranet/@@users-settings"
        icon_expr="string:${portal_url}/++resource++younglives.content.images/icon_intranet_users.png"
        visible="True"
        i18n:attributes="title">
        <permission>Manage portal</permission>
    </configlet>

</object>